	@EventHandler // While MC Runs
    public void init(FMLInitializationEvent event){
		ItemStack LapisStack = new ItemStack(Items.dye, 1, 4);
		ItemStack HardenedIronIngotStack = new ItemStack(itemHardenedIronIngot, 1);
		ItemStack ReinforcedIronIngotStack = new ItemStack(itemReinforcedIronIngot);
		
		GameRegistry.addShapelessRecipe(HardenedIronIngotStack, new Object[] {Items.iron_ingot, itemCompressedCoal});
		GameRegistry.addShapelessRecipe(new ItemStack(itemHardenedLapisChunk, 1), new Object[] {itemCompressedLapis, itemCompressedCoal});
		GameRegistry.addShapelessRecipe(new ItemStack(itemHardenedQuartzChunk, 1), new Object[] {itemCompressedQuartz, itemCompressedCoal});
		GameRegistry.addShapelessRecipe(new ItemStack(itemNetheriumIngot, 1), new Object[] {Items.netherbrick, itemHardenedIronIngot});
		GameRegistry.addShapelessRecipe(new ItemStack(itemReinforcedLapisIngot, 1), new Object[] {itemHardenedLapisChunk, itemHardenedIronIngot});
		GameRegistry.addShapelessRecipe(new ItemStack(itemReinforcedObsidianIngot, 1), new Object[] {Blocks.obsidian, itemHardenedIronIngot});
		GameRegistry.addShapelessRecipe(new ItemStack(itemReinforcedQuartzIngot, 1), new Object[] {itemHardenedQuartzChunk, itemHardenedIronIngot});
		
		/**GameRegistry.addShapedRecipe(new ItemStack(itemRecurveLimb, 2), new Object[] {"XXY", Character.valueOf('X'), itemHardenedIronIngot, Character.valueOf('Y'), itemCompressedCoal});
		GameRegistry.addShapedRecipe(new ItemStack(itemNetheriumRecurveLimb, 1), new Object[] {"Y", "X", Character.valueOf('X'), itemRecurveLimb, Character.valueOf('Y'), Character.valueOf('Y'), itemNetheriumIngot});
		GameRegistry.addShapedRecipe(new ItemStack(itemReinforcedIronRecurveLimb, 1), new Object[] {"Y", "X", Character.valueOf('X'), itemRecurveLimb, Character.valueOf('Y'), Character.valueOf('Y'), itemReinforcedIronIngot});
		GameRegistry.addShapedRecipe(new ItemStack(itemReinforcedLapisRecurveLimb, 1), new Object[] {"Y", "X", Character.valueOf('X'), itemRecurveLimb, Character.valueOf('Y'), Character.valueOf('Y'), itemReinforcedLapisIngot});
		GameRegistry.addShapedRecipe(new ItemStack(itemReinforcedObsidianRecurveLimb, 1), new Object[] {"Y", "X", Character.valueOf('X'), itemRecurveLimb, Character.valueOf('Y'), Character.valueOf('Y'), itemReinforcedObsidianIngot});
		GameRegistry.addShapedRecipe(new ItemStack(toolNetheriumBow, 1), new Object[] {"X  ", " Y ", "  X", Character.valueOf('X'), itemNetheriumRecurveLimb, Character.valueOf('Y'), Character.valueOf('Y'), Items.bow});		
		GameRegistry.addShapedRecipe(new ItemStack(toolReinforcedIronBow, 1), new Object[] {"X  ", " Y ", "  X", Character.valueOf('X'), itemReinforcedIronRecurveLimb, Character.valueOf('Y'), Character.valueOf('Y'), Items.bow});
		GameRegistry.addShapedRecipe(new ItemStack(toolReinforcedLapisBow, 1), new Object[] {"X  ", " Y ", "  X", Character.valueOf('X'), itemReinforcedLapisRecurveLimb, Character.valueOf('Y'), Character.valueOf('Y'), Items.bow});
		GameRegistry.addShapedRecipe(new ItemStack(toolReinforcedObsidianBow, 1), new Object[] {"X  ", " Y ", "  X", Character.valueOf('X'), itemReinforcedObsidianRecurveLimb, Character.valueOf('Y'), Character.valueOf('Y'), Items.bow});*/

		GameRegistry.addShapedRecipe(new ItemStack(itemCompressedCoal, 1), new Object[] {"XXX", "X X", "XXX", Character.valueOf('X'), Items.coal});
		GameRegistry.addShapedRecipe(new ItemStack(itemCompressedLapis, 1), new Object[] {"XXX", "X X", "XXX", Character.valueOf('X'), LapisStack});
		GameRegistry.addShapedRecipe(new ItemStack(itemCompressedQuartz, 1), new Object[] {"XXX", "X X", "XXX", Character.valueOf('X'), Items.quartz});
		
		GameRegistry.addShapedRecipe(new ItemStack(toolNetheriumPickaxe, 1), new Object[] {"XXX", " Y ", " Y ", Character.valueOf('X'), itemNetheriumIngot, Character.valueOf('Y'), Items.stick});
		GameRegistry.addShapedRecipe(new ItemStack(toolReinforcedIronPickaxe, 1), new Object[] {"XXX", " Y ", " Y ", Character.valueOf('X'), itemReinforcedIronIngot, Character.valueOf('Y'), Items.stick});
		GameRegistry.addShapedRecipe(new ItemStack(toolReinforcedLapisPickaxe, 1), new Object[] {"XXX", " Y ", " Y ", Character.valueOf('X'), itemReinforcedLapisIngot, Character.valueOf('Y'), Items.stick});
		GameRegistry.addShapedRecipe(new ItemStack(toolReinforcedObsidianPickaxe, 1), new Object[] {"XXX", " Y ", " Y ", Character.valueOf('X'), itemReinforcedObsidianIngot, Character.valueOf('Y'), Items.stick});
		
		GameRegistry.addShapedRecipe(new ItemStack(toolNetheriumAxe, 1), new Object[] {" XX", " YX", " Y ", Character.valueOf('X'), itemNetheriumIngot, Character.valueOf('Y'), Items.stick});
		GameRegistry.addShapedRecipe(new ItemStack(toolReinforcedIronAxe, 1), new Object[] {" XX", " YX", " Y ", Character.valueOf('X'), itemReinforcedIronIngot, Character.valueOf('Y'), Items.stick});
		GameRegistry.addShapedRecipe(new ItemStack(toolReinforcedLapisAxe, 1), new Object[] {" XX", " YX", " Y ", Character.valueOf('X'), itemReinforcedLapisIngot, Character.valueOf('Y'), Items.stick});
		GameRegistry.addShapedRecipe(new ItemStack(toolReinforcedObsidianAxe, 1), new Object[] {" XX", " YX", " Y ", Character.valueOf('X'), itemReinforcedObsidianIngot, Character.valueOf('Y'), Items.stick});
		GameRegistry.addShapedRecipe(new ItemStack(toolNetheriumAxe, 1), new Object[] {"XX ", "XY ", " Y ", Character.valueOf('X'), itemNetheriumIngot, Character.valueOf('Y'), Items.stick});
		GameRegistry.addShapedRecipe(new ItemStack(toolReinforcedIronAxe, 1), new Object[] {"XX ", "XY ", " Y ", Character.valueOf('X'), itemReinforcedIronIngot, Character.valueOf('Y'), Items.stick});
		GameRegistry.addShapedRecipe(new ItemStack(toolReinforcedLapisAxe, 1), new Object[] {"XX ", "XY ", " Y ", Character.valueOf('X'), itemReinforcedLapisIngot, Character.valueOf('Y'), Items.stick});
		GameRegistry.addShapedRecipe(new ItemStack(toolReinforcedObsidianAxe, 1), new Object[] {"XX ", "XY ", " Y ", Character.valueOf('X'), itemReinforcedObsidianIngot, Character.valueOf('Y'), Items.stick});
		
		GameRegistry.addShapedRecipe(new ItemStack(toolNetheriumShovel, 1), new Object[] {"X", "Y", "Y", Character.valueOf('X'), itemNetheriumIngot, Character.valueOf('Y'), Items.stick});
