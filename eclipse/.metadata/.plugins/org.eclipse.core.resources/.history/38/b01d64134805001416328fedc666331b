package com.orcinuss.reinforcedtools.item.tools;

import com.orcinuss.reinforcedtools.creativetab.CreativeTab;

import net.minecraft.item.EnumRarity;
import net.minecraft.item.Item;
import net.minecraft.item.ItemPickaxe;

public class ItemRTMPickaxeTest extends ItemPickaxe{
	public EnumRarity rarity;
	
	public ItemRTMPickaxeTest(Item.ToolMaterial material){
		this(material, EnumRarity.common);
	}
	
	public ItemRTMPickaxeTest(Item.ToolMaterial material, EnumRarity rarity) {
		super(material);
		this.maxStackSize = 1;
		this.setCreativeTab(CreativeTab.RTM_TAB);
		this.rarity = rarity;
	}

}
